<!DOCTYPE html>
<html>
<head>
  <title>RailsTemplate</title>
  <%= csrf_meta_tags %>

  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
  <%= stylesheet_link_tag "https://fonts.googleapis.com/css?family=Roboto:300,400,500,700|Roboto+Slab:400,700|Material+Icons" %>

  <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
</head>

<body>
  <div class="wrapper">
    <%= render partial: 'layouts/shared/leftnav' %>
  </div>
  <div class="main-panel">
  	<%= render partial: 'layouts/shared/topbar' %>
  	<div class="content">
    <div class="container-fluid">
<%= yield %>
	<%#= render partial: 'layouts/shared/settings' %>
</div>
</div>
</body>


</html>
